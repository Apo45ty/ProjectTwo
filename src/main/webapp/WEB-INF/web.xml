<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>GetAllRequest</servlet-name>
		<display-name>GetAllRequest</display-name>
		<description></description>
		<servlet-class>com.revature.http.GetAllRequest</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>RunSelenium</servlet-name>
		<display-name>RunSelenium</display-name>
		<description></description>
		<servlet-class>com.revature.http.RunSelenium</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetAllRequest</servlet-name>
		<url-pattern>/getAll/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RunSelenium</servlet-name>
		<url-pattern>/RunSelenium/</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
